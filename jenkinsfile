pipeline {
    agent {
        label 'worker'
    }
    environment {
        DOCKER_REGISTRY = "docker.io"
        DOCKER_IMAGE_NAME = "070387/jenkins"
    }
    triggers {
        pollSCM('*/5 * * * *')
    }
    stages {
        stage('Build') {
            steps {
                sh "docker build -t 070387 docker"
            }
        }
    }
}

